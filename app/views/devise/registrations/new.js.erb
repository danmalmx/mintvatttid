let signupForm = "<%= escape_javascript(render partial: 'partials/signup') %>"
let displayElement = document.getElementById('user-forms')
displayElement.innerHTML = signupForm;

let modal = document.getElementById('modal')
window.onclick = function(event) {
        if (event.target == modal) {
            modal.style.display = "none";
            location.reload()
        }
    }