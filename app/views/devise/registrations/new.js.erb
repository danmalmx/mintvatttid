let loginForm = "<%= escape_javascript(render partial: 'signup') %>"
let displayElement = document.getElementById('user-forms')
displayElement.innerHTML = loginForm;

let modal = document.getElementById('modal')
let btn = document.getElementById('btn')
window.onclick = function(event) {
        if (event.target == modal) {
            modal.style.display = "none";
            location.reload()
        }
    }